<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aaa.group_three.dao.BBusinessInfoMapper">

    <select id="findAll" resultType="com.aaa.entity.BBusinessInfo">
        select
            sname,bname,license,telephone,cardfront,cardreverse,sortname,reason,b_business_info.status
        from b_business_info,g_sort
        where b_business_info.sortid=g_sort.id
    </select>
</mapper>
