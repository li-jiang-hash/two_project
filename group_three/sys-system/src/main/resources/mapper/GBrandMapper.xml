<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aaa.group_three.dao.GBrandMapper">

    <select id="getAll" resultType="java.util.Map">
        select g_brand.id,brandname,sortname,g_brand.check_content,g_brand.status
        from g_brand,g_sort
        where g_brand.id=g_sort.id and g_brand.is_deleted=0
<!--        <if test="brands.id!=null">-->
<!--            and g_brand.id=#{brands.id}-->
<!--        </if>-->
    </select>
</mapper>
